{
  "name": "sunnysingh.io",
  "version": "1.0.0",
  "type": "module",
  "description": "Personal Website of Sunny Singh",
  "author": "Sunny Singh <me@sunnysingh.io>",
  "homepage": "https://sunnysingh.io/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunnysingh/sunnysingh.io.git"
  },
  "engines": {
    "node": ">=24",
    "npm": ">=11"
  },
  "scripts": {
    "dev": "npm run generate && greenwood develop",
    "build": "greenwood build",
    "serve": "greenwood serve",
    "generate": "npm run generate:vscode-custom-data && npm run generate:component-docs",
    "postgenerate": "prettier src/components/README.md .vscode/html-custom-data.json --write",
    "generate:component-docs": "wca analyze src --format markdown --outFile src/components/README.md",
    "generate:vscode-custom-data": "wca analyze src --format vscode --outFile .vscode/html-custom-data.json",
    "checks": "npm run typecheck && npm run lint:check && npm run format:check",
    "typecheck": "tsc",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "dependencies": {
    "open-props": "1.7.17"
  },
  "devDependencies": {
    "@eslint/css": "0.14.1",
    "@eslint/js": "9.39.2",
    "@eslint/json": "0.14.0",
    "@eslint/markdown": "7.5.1",
    "@greenwood/cli": "0.33.3",
    "@html-eslint/eslint-plugin": "0.52.1",
    "@html-eslint/parser": "0.52.0",
    "eslint": "9.39.2",
    "eslint-plugin-wc": "3.0.2",
    "jiti": "2.6.1",
    "prettier": "3.7.4",
    "typescript": "5.9.3",
    "typescript-eslint": "8.51.0",
    "web-component-analyzer": "2.0.0"
  }
}
